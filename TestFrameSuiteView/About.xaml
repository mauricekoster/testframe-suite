<Window x:Class="TestFrameSuiteView.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="About" Height="400" Width="600">
    <Grid Background="LightGray">
        <Grid.RowDefinitions>
            <RowDefinition Height="87*" />
            <RowDefinition Height="274*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="130*" />
            <ColumnDefinition Width="150" />
        </Grid.ColumnDefinitions>
        <StackPanel Grid.Column="1" Grid.Row="1">
            <Button Content="Close" Margin="10" Name="btnClose" Height="23" Click="btnClose_Click" />
        </StackPanel>
        <Image Height="81" HorizontalAlignment="Left" Margin="18,181,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="122" Grid.Column="1" Source="/TestFrameSuiteView;component/Images/cgi-logo.png" Grid.Row="1" />
        <Image Height="84" HorizontalAlignment="Left" Name="image2" Stretch="Fill" VerticalAlignment="Top" Width="578" Source="/TestFrameSuiteView;component/Images/homepage-banner-1.jpg" Grid.ColumnSpan="2" />
        <TextBlock Grid.Row="1" Height="244" HorizontalAlignment="Left" Margin="14,15,0,0" Name="txtVersionInfo" Text="" VerticalAlignment="Top" Width="399" />
    </Grid>
</Window>
